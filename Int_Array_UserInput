//Created a java program to take user input of elements of an array of int Type and then print it.

package Array_Programs;

import java.util.Scanner;											//imported Scanner class to take user input

public class Int_Array_UserInput 
{
	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		
		int a[]=new int[6];		    								//Declared an array of int type	
		
		System.out.println("Enter elements of array of size "+a.length+" : - ");
		for(int i=0;i<a.length;i++)									//using loop we can initialize values 
		{
			a[i]=sc.nextInt();
		}
		for(int j=0;j<a.length;j++)									//using loop for printing the values
		{
			System.out.println("a["+j+"] = "+a[j]);
		}
	}
}
